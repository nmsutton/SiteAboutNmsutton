			$(document).ready(function() {
				$('#myCanvas').mousemove(function(event) {
					adjustedMouseX = (event.pageX + 2 - 50) * 0.001;
					adjustedMouseY = (event.pageY - 210 - 300) * 0.001;
					cube1.rotation.x = adjustedMouseY;
					cube1.rotation.y = adjustedMouseX;
					adjustedMouseX = (event.pageX + 2 - 50) * 0.003;
					adjustedMouseY = (event.pageY - 210 - 300) * 0.003;
					cube2.rotation.x = adjustedMouseY;
					cube2.rotation.y = adjustedMouseX;
					adjustedMouseX = (event.pageX + 2 - 50) * 0.005;
