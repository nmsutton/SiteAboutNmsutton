		<script src="http://www.html5canvastutorials.com/libraries/three.min.js"></script>
		<script>
			//<script defer="defer">
			adjustedMouseX = 0;
			adjustedMouseY = 0;
			var canvasWidth = 100;
			var canvasHeight = 800;

			// revolutions per second
			var angularSpeed = 0.2;
			var lastTime = 0;

			// this function is executed on each animation frame
			function animate() {
				// update
				var time = (new Date()).getTime();
				var timeDiff = time - lastTime;
				var angleChange = angularSpeed * timeDiff * 2 * Math.PI / 1000;

				/*$(document).ready(function() {
				$('#myCanvas').mousemove(function(event) {
				adjustedMouseX = event.pageX - 53;
				adjustedMouseY = event.pageY - 10;
				cube.rotation.y = adjustedMouseY;
				}
				}*/

				//cube.rotation.y += angleChange;
				lastTime = time;

				// render
				renderer.render(scene, camera);

				// request new frame
				requestAnimationFrame(function() {
					animate();
				});
			}

			// renderer
			var renderer = new THREE.CanvasRenderer();
			renderer.setSize(canvasWidth, canvasHeight);
			document.body.appendChild(renderer.domElement);

			// camera
			var camera = new THREE.PerspectiveCamera(45, canvasWidth / canvasHeight, 1, 1000);
			camera.position.z = 900;

			// scene
			var scene = new THREE.Scene();

			// cube
			var cube1 = new THREE.Mesh(new THREE.CubeGeometry(50, 50, 50), new THREE.MeshNormalMaterial());
			cube1.overdraw = true;
			cube1.position.y = 300;
			scene.add(cube1);

			var cube2 = new THREE.Mesh(new THREE.CubeGeometry(50, 50, 50), new THREE.MeshNormalMaterial());
			cube2.overdraw = true;
			cube2.position.y = 150;
			scene.add(cube2);

			var cube3 = new THREE.Mesh(new THREE.CubeGeometry(50, 50, 50), new THREE.MeshNormalMaterial());
			cube3.overdraw = true;
			scene.add(cube3);

			var cube4 = new THREE.Mesh(new THREE.CubeGeometry(50, 50, 50), new THREE.MeshNormalMaterial());
			cube4.overdraw = true;
			cube4.position.y = -150;
			scene.add(cube4);

			var cube5 = new THREE.Mesh(new THREE.CubeGeometry(50, 50, 50), new THREE.MeshNormalMaterial());
			cube5.overdraw = true;
			cube5.position.y = -300;
			scene.add(cube5);

			// start animation
			animate();

			/*$(document).ready(function() {
			 $('#myCanvas').mousemove(function(event) {
